[color]
	ui = true
[column]
	ui = auto
[branch]
	sort = -committerdate
[tag]
	sort = version:refname
[core]
	excludesfile = /Users/rwjpeelen/.gitignore_global
	autocrlf = false
	ignorecase = false
	sparseCheckout = false
[user]
	name = Roland Peelen
	email = rolandpeelen@gmail.com
	signingkey = E4FD3A3A9BDE4CE4
[diff]
	algorithm = histogram
	colorMoved = plain
	mnemonicPrefix = true
	renames = true
[push]
	default = upstream
	autoSetupRemote = true
	followTags = true
[fetch]
	prune = true
	pruneTags = true
	all = true
[commit]
	template = ~/.stdCommitMessage
	gpgsign = true
[init]
	defaultBranch = master
[pull]
	ff = only
[merge]
	conflictstyle = zdiff3
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	prequired = true
[rerere]
	enabled = true
